<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xiaoshabao.base.entity.SysConfigEntity">
	<select id="getSysConfigEntityById" resultType="com.xiaoshabao.base.entity.SysConfigEntity">
		SELECT ID,NAME,TYPE,VALUE,REMARK FROM sys_config WHERE ID=#{id}
	</select>
</mapper>